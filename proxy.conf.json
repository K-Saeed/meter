{
  "/api/*": {
    "target": "https://dashboard-dev.meter.com.sa",
    "secure": false,
    "logLevel": "debug",
    "headers": { "X-Forwarded-Prefix": "/api/" }
  },
  "/socket.io": {
    "target": "ws://localhost:8000",
    "ws": true,
    "secure": false,
    "logLevel": "debug",
    "changeOrigin": true
  }
}